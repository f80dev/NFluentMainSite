{
  "name": "nfluent-web-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start local 4202": "ng serve --port 4202 --live-reload",
    "start local": "ng serve --port 4200 --host 0.0.0.0 --disable-host-check --live-reload",
    "start debug": "ng serve --configuration debug --live-reload",
    "build": "ng build",
    "deploy": "ng build --aot --output-hashing=all --configuration production && gh-pages -d ./docs --repo https://github.com/f80dev/WebSiteNFluent.git -f -t true -b gh-pages -m \"update from gh-pages\"",
    "deploy_debug": "ng build --configuration debug --aot --output-hashing=all && gh-pages -d ./docs --repo https://github.com/f80dev/WebSiteNFluent.git -f -t true -b gh-pages -m \"update from gh-pages\"",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@abacritt/angularx-social-login": "^1.2.5",
    "@angular/animations": "15.2.4",
    "@angular/cdk": "15.2.4",
    "@angular/common": "15.2.4",
    "@angular/compiler": "15.2.4",
    "@angular/core": "15.2.4",
    "@angular/forms": "15.2.4",
    "@angular/material": "15.2.4",
    "@angular/platform-browser": "15.2.4",
    "@angular/platform-browser-dynamic": "15.2.4",
    "@angular/router": "15.2.4",
    "@elrondnetwork/erdjs": "^11.1.0",
    "@elrondnetwork/erdjs-wallet-connect-provider": "^2.0.3",
    "@elrondnetwork/erdjs-web-wallet-provider": "^2.1.1",
    "@nicky-lenaers/ngx-scroll-to": "14.0.0",
    "@solana/spl-token": "^0.3.5",
    "@solana/web3.js": "^1.63.1",
    "animejs": "^3.2.1",
    "gh-pages": "4.0.0",
    "hammerjs": "^2.0.8",
    "jsqr": "^1.4.0",
    "load-yaml-file": "^1.0.0",
    "ng-gallery": "^8.0.3",
    "ngx-socket-io": "^4.4.0",
    "ngx-webcam": "^0.4.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.4.0",
    "yaml": "^2.1.3",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.4",
    "@angular/cli": "15.2.4",
    "@angular/compiler-cli": "15.2.4",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@types/node-fetch": "^2.6.2",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.8.4"
  }
}
